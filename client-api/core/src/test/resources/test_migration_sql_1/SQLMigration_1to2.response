SELECT CAST(_entity."ID" as TEXT) as "URI" , ("_entity_a")."i" AS "ai", ("_entity_a")."s" AS "as", ("_entity_a")."f" AS "af", ("_entity")."i" AS "i", ("_entity")."s" AS "s", ("_entity")."f" AS "f"
