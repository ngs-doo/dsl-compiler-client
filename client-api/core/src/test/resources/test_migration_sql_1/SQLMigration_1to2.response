	"myModule"."B_entity" _entity
	
	
	INNER JOIN "myModule"."A_entity" "_entity_a" ON "_entity_a"."ID" = "_entity"."aID";
COMMENT ON VIEW "myModule"."snowBA_snowflake" IS 'NGS volatile';

